#! c:/iverilog-x86/bin/vvp
:ivl_version "10.0 (stable)" "(v10_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01083800 .scope module, "stimulus_while" "stimulus_while" 2 1;
 .timescale 0 0;
v01085228_0 .var/i "count", 31 0;
    .scope S_01083800;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v01085228_0, 0, 32;
T_0.0 ;
    %load/vec4 v01085228_0;
    %cmpi/s 128, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v01085228_0;
    %addi 1, 0, 32;
    %store/vec4 v01085228_0, 0, 32;
    %vpi_call 2 11 "$display", "Count = %d", v01085228_0 {0 0 0};
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "while.v";
